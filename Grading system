                     /*Program for grading system of a student*/
#include<stdio.h>
void main()
{
    int marks;
    printf("Enter your marks ");         //User can input its marks
    scanf("%d",&marks);
    if(marks<0 || marks>100)             //In case if user will input number less than  
    {                                    //0 or greater than 100 then the wrong
        printf("Wrong Entry");           //entry will be displayed
    }
    else if(marks<40)
    {
        printf("Grade F");
    }
    else if(marks>=40 && marks<=54)
    {
        printf("Grade D");
    }
    else if(marks>=55 && marks<69)
    {
        printf("Grade C");
    }
    else if(marks>=70 && marks<=84)
    {
        printf("Grade B");
    }
    else if(marks>=85 && marks<=100)
    {
        printf("Grade A");
    }
    
}

